#include <stdio.h>
int main() {
    int a;
    scanf("%d",&a);
    int ar[a];  
    for(int i=0;i<a;i++)
    {
        scanf("%d",&ar[i]);
    }
    for(int i=0;i<a;i++)
    {
        int c=0;
        for(int j=i;j<a;j++)
        {
            if(ar[i]==ar[j])
            {
            c++;
            }
        }
    if (c==2 && ar[i]%2==0)
    {
       int x=ar[i];
       printf("%d",x);
    }
}
