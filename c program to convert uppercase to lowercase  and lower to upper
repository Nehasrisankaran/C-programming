#include <stdio.h>
#include<string.h>
int main() {
    char str [50];
    char str1[50];
    scanf("%s",str);
    for(int i=0;str[i]!='\0';i++)
    {
        if (str[i]>=97&&str[i]<=122) //lwr replace 97 to 65 n 122 to 90
        {
         str[i]=str[i]-32; //fr lwr   -32
        }
        else if (str[i]>=65&&str[i]<=90) //lwr replace 97 to 65 n 122 to 90
        {
         str[i]=str[i]+32; //fr lwr   -32
        }
    }
    printf("%s",str);
}
