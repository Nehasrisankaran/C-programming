#include <stdio.h>
#include<string.h>
int main() {
    char str [50];
   // char str1[50];
    scanf("%s",str);
    for(int i=0;str[i]!='\0';i++)
    {
        if(str[i]==65||str[i]==69||str[i]==73||str[i]==79||str[i]==85)
        {
            printf("vowels\n");
        }
        else 
        {
            printf("consonants");
        }
    }
}
