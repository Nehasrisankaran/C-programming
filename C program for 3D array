#include <stdio.h>

int main() {
    int a,b;
    scanf("%d%d",&a,&b);
    int ar[a][b][b];
    for(int i=0;i<a;i++)
    {
        for(int j=0;j<b;j++)
        {
            for(int k=0;k<b;k++)
            {
                scanf("%d",&ar[i][j][k]);
            }
        int x,y,z;
        printf("welcome to mec cine\n");
        printf("enter the screen number:\n");
        scanf("%d",&x);
        printf("enter row num\n");
        scanf("%d",&y);
        printf("enter seat num\n:");
        scanf("%d",&z);
        
    }
