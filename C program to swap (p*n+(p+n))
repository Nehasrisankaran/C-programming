#include <stdio.h>
int main() {
   int a;
   scanf("%d",&a);
   int ar[a];
   for(int i=0;i<a;i++)
   {
       scanf("%d",&ar[i]);
   }
   int pv=0,nv=0;
   for(int i=0;i<a;i++)
   {
       if(ar[i]>0)
       {
       pv++;
   }
   else if (ar[i]<0)
   {
      nv++;
   }
   }
   printf("%d",((pv*nv)+pv+nv));
    return 0;
}
